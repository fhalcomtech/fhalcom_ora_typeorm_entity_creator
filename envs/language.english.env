# COMMENTS TO fhalcom.config.json
FHALCOM_DB_HOST_COMMENT = "Database host, by default it is localhost or you can specify one, like this 192.168.1.100"
FHALCOM_DB_PORT_COMMENT = "Database port, by default oracle uses port 1521"
FHALCOM_DB_USER_COMMENT = "Username to connect to the database"
FHALCOM_DB_PASSWORD_COMMENT = "Access key to the database"
FHALCOM_DB_DBNAME_COMMENT = "The name of the database in oracle known as in oracle SID = identifies the database instance (database name + instance number) example: dbname01"
FHALCOM_DB_SCHEMAS_COMMENT = "Arrangement of strings with the names of the schemes to search for ['s1', 's2']. Required if you don't specify tables"
FHALCOM_DB_TABLES_COMMENT = "Array of objects that must contain schema and table such as [{schema: s1, table: t1}]. Required if you don't specify schema"
FHALCOM_DB_EXCLUDE_COMMENT = "Array of objects that must contain schema and table such as [{schema: s1, table: t1}], It tells us that tables will not be taken into account. Mandatory is not mandatory"